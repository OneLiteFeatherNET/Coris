{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":semanticCommitsDisabled"
  ],
  "labels": ["Renovate"],
  "rebaseWhen": "conflicted",
  "packageRules": [
    {
      "matchUpdateTypes": [
        "patch"
      ],
      "automerge": true
    },
    {
      "matchUpdateTypes": [
        "minor"
      ],
      "automerge": false,
      "schedule": [
        "before 6:00am every friday"
      ]
    }
  ]
}